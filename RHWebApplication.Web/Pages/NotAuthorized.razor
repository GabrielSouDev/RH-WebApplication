@inject NavigationManager NavigationManager
@page "/NotAuthorized"
<PageTitle>Not Authorized</PageTitle>
<MudPaper>
    <MudText Typo="Typo.h4" Style="height:10vh;">Não autorizado, faça o login para prosseguir.</MudText>
    <MudButton OnClick="GoToLogin">Login</MudButton>
</MudPaper>
@code {
    private void GoToLogin()
    {
        NavigationManager.NavigateTo("/Login");
    }
}
